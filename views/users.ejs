<%- include('includes/header.ejs') %>

<h1><%= title %></h1>
<p>Welcome to <%= title %></p>
<% if(users.length > 0 ) { %>
<div class="users-list">
    <ul>
        <% users.forEach(user => { %>
        <li><a href="/user/<%= user.id %>"><%= user.username %></a></li>
        <% })%>
    </ul>
</div>
<% } %>
<%- include('includes/footer.ejs') %>